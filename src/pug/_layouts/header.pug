// ========== Header Start ==========
header.l-header#js-header
  div.container
    h2 ヘッダー
    nav.l-header__nav.global-nav
      ul.global-nav__list.d-flex.justify-content-between
        each k, v in pages
          if pageID == v
            li.global-nav__item.global-nav__item--current #{pages[v]['title']}
          else
            li.global-nav__item: a.global-nav__link(href=`/${k.path}`) #{pages[v]['title']}
// ==========  Header End ==========
